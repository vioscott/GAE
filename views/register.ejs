    <form class="login-container" method="post">
        <p><input type="email" name="email" placeholder="Email" required></p>
        
        <p><input type="text" name="username" placeholder="Create Unique Username" required></p>

        <p><input type="password" name="password" placeholder="Password" required></p>

        <p><input type="password" name="password2" placeholder="Repeat Password" required></p>

        <p><input type="submit" name="submit" value="Sign Up"></p>

    </form>

    <p>ALready Signed Up? <a href="/login" class="text-white">Login</a></p>
</div>

